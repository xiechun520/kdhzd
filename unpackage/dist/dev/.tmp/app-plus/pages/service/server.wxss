
.page {
	height: 100vh;
}
.fw-tab{
	border-radius: 10rpx;
	overflow: hidden;
	-webkit-box-shadow: 0rpx 0rpx 6rpx 4rpx #f1f1f1;
	        box-shadow: 0rpx 0rpx 6rpx 4rpx #f1f1f1;
	margin: 20rpx;
}
.fw-tab view{
	width: 50%;
	height: 80rpx;
	line-height: 80rpx;
	text-align: center;
}
view.active{
	background: rgba(50,194,255,1);
	color: #ffffff;
}
view.active1{
	background: #42b983;
	color: #ffffff;
}
.server-title{
	padding: 20rpx 10rpx;
	border-radius: 10rpx 10rpx 0rpx 0rpx;
}
.item {
	width: 80rpx;
	height: 80rpx;
	background: rgba(255, 255, 255, 1);
	-webkit-box-shadow: 0rpx 0rpx 9rpx 1rpx rgba(0, 0, 0, 0.1);
	        box-shadow: 0rpx 0rpx 9rpx 1rpx rgba(0, 0, 0, 0.1);
	border-radius: 50%;
}
.list {
	width: 698rpx;
	height: 259rpx;
	background: rgba(255, 255, 255, 1);
	border-radius: 15rpx;
}
.list3 {
	width: 698rpx;
	background: rgba(255, 255, 255, 1);
	border-radius: 15rpx;
}
.cu-item{
	width: 48%;
	-webkit-box-shadow: 0rpx 1px 4rpx 4rpx #eeeeee;
	        box-shadow: 0rpx 1px 4rpx 4rpx #eeeeee;
	margin-bottom: 20rpx;
	border-radius: 20rpx;
}

