<view><cu-custom vue-id="1" bgColor="bg-thame" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">添加团队</view></cu-custom><view class="input-group card-menus"><view class="input-row border"><text class="title">团队名称：</text><input class="m-input" type="text" clearable focus placeholder="请输入团队名称" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="input-row border"><text class="title">团队所属：</text><input class="m-input" type="text" clearable placeholder="请输入团队所属" data-event-opts="{{[['input',[['__set_model',['','suoshu','$event',[]]]]]]}}" value="{{suoshu}}" bindinput="__e"/></view><view class="input-row border"><text class="title">团队地址：</text><input class="m-input" type="text" clearable placeholder="请输入团队地址" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view><view class="feedback margin-top-sm padding-sm"><textarea focus placeholder-style="color:#888888;line-height: 23px;" placeholder="请描述团队擅长" data-event-opts="{{[['input',[['__set_model',['','best','$event',[]]]]]]}}" value="{{best}}" bindinput="__e"></textarea><view class="imglist"><block wx:for="{{imageList}}" wx:for-item="image" wx:for-index="index" wx:key="index"><view><text data-event-opts="{{[['tap',[['delimg',[index]]]]]}}" class="cuIcon-roundclose" bindtap="__e"></text><image class="uni-uploader__img" src="{{image}}" mode="aspectFill" data-src="{{image}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image></view></block></view><view class="padding"></view><image style="width:160rpx;height:160rpx;" src="../../static/addimg.png" mode="aspectFit" data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="padding-top"></view><button class="cu-btn lg bg-thame block margin-sm" type data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></view>