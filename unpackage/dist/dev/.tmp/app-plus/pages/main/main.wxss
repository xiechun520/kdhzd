.anima{
	font-size: 34rpx;
}
.anima text{
	margin: 0px 4rpx;
}
.anima text:first-child{
	-webkit-animation:mymove 5s infinite;
	        animation:mymove 5s infinite;
}
.anima text:nth-child(2){
	-webkit-animation:mymove1  5s infinite 0.5s;
	        animation:mymove1  5s infinite 0.5s;
}
.anima text:nth-child(3){
	-webkit-animation:mymove2  5s infinite 1s;
	        animation:mymove2  5s infinite 1s;
}
@-webkit-keyframes mymove{
0%   {color:red;}
25%  {color:yellow;}
50%  {color:blue;}
75%  {color:block;}
100% {color:red;}
}
@keyframes mymove{
0%   {color:red;}
25%  {color:yellow;}
50%  {color:blue;}
75%  {color:block;}
100% {color:red;}
}
@-webkit-keyframes mymove1{
0%   {color:red;}
25%  {color:yellow;}
50%  {color:blue;}
75%  {color:block;}
100% {color:red;}
}
@keyframes mymove1{
0%   {color:red;}
25%  {color:yellow;}
50%  {color:blue;}
75%  {color:block;}
100% {color:red;}
}
@-webkit-keyframes mymove2{
0%   {color:red;}
25%  {color:yellow;}
