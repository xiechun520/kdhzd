
.pickerdata {
	width: 100%;
	padding: 30rpx;
}
.pickerafter {
	position: fixed;
	z-index: 1;
	width: 100%;
	height: 50%;
	overflow-y: scroll;
}
.scroll-view_H {
	white-space: nowrap;
	width: 100%;
}
.sttime {
	position: relative;
	margin: 10rpx 0rpx;
	width: 19%;
}
.sttime input {
	width: 100%;
	border: 2rpx solid #f1f1f1;
	border-radius: 6rpx;
	padding: 0rpx 6rpx;
	height: 50rpx;
	line-height: 50rpx;
	font-size: 28rpx;
}
.xztime {
	position: fixed;
	bottom: -100%;
	left: 0rpx;
	z-index: 9;
	width: 100%;
	height: 50%;
	padding: 30rpx;
	text-align: center;
	border: 2rpx solid #f1f1f1;
	border-top: none;
	background: #ffffff;
	z-index: 99;
	-webkit-transition: all 0.5s;
	-o-transition: all 0.5s;
	transition: all 0.5s;
}
.xztime1{
	overflow-y: scroll;
	height: 100%;
}
.xztime1 view{
	line-height: 70rpx;
	border-bottom: 2rpx solid #f1f1f1;
}
.action:before{
	position: absolute;
	left: 0rpx;
	top: -100%;
	content: '';
	width: 100%;
	height: 100%;
	background: rgb(0,0,0,0.8);
}
.action{
	bottom: 0%;
}
.active {
	color: #0081FF;
	font-size: 32rpx;
}
.uni-input-placeholder {
	color: #888888;
	font-size: 28rpx;
}
.pblb view{
	width: 18%;
	font-size: 26rpx;
}
.pblb view:first-child,
.pblb view:last-child{
	width: 12%;
}
.yjpb{
	position: fixed;
	top: 200%;
	left: 0%;
	width: 100%;
	height: 100%;
	background: #FFFFFF;
	z-index: 10;
	padding: 30rpx;
	-webkit-transition: all 0.5s;
	-o-transition: all 0.5s;
	transition: all 0.5s;
}
.act{
	top: 7%;
}

